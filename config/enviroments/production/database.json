{
    "defaultConnection": "default",
    "connections": {
      "default": {
        "connector": "bookshelf",
        "settings": {
          "client": "postgres",
          "host": "${process.env.DATABASE_HOST || 'localhost'}",
          "port": "${process.env.DATABASE_PORT || 27017}",
          "database": "${process.env.DATABASE_NAME || 'sammy'}",
          "username": "${process.env.DATABASE_USERNAME || 'sammy'}",
          "password": "${process.env.DATABASE_PASSWORD || 'Admin@123'}"
        },
        "options": {
          "ssl": false
        }
      }
    }
  }